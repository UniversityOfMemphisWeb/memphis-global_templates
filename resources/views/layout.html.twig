{% if app.request.query.get('alert') is not null %}
        <div class="header--alert" style="position: absolute; z-index: 999;">
            <div class="container">
                <div class="row">
                    <div class="col-md-16 col-md-offset-8">
                        <div class="header--alert__wrapper">
                            <div class="header--alert__wrapper__icon">
                                <i class="fa fa-exclamation-circle"></i>
                            </div>
                            <div class="header--alert__wrapper__text">
                                <span>University closed due to inclement weather</span>
                                <a href="#" style="color: #{{ app.request.query.get('color') }} !important;">More <i class="fa fa-chevron-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script type="text/javascript">
            jQuery(document).ready(function(){
                jQuery('header').css('top', '87px');
                jQuery('div.type--home').css('margin-top', '69px');
            });
        </script>
{% endif %}

